{
  "name": "kd",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "prisma generate && vite build",
    "preview": "vite preview",
    "start": "yarn build && node ./server",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-organize-imports --plugin=prettier-plugin-tailwindcss --check . && eslint .",
    "format": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-organize-imports --plugin=prettier-plugin-tailwindcss --write ."
  },
  "imports": {
    "$components": {
      "default": "./src/components/*/"
    },
    "$static": {
      "default": "./static/*/"
    },
    "$assets": {
      "default": "./src/assets/*/"
    },
    "$lib": {
      "default": "./src/lib/index.js"
    },
    "$lib/server": {
      "default": "./src/lib/server/index.js"
    }
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.22.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.196",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.27",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "prisma": "^5.0.0",
    "supabase": "^1.88.0",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  },
  "type": "module",
  "packageManager": "yarn@3.2.4",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "@rgossiaux/svelte-headlessui": "^2.0.0",
    "@types/pg": "^8.10.2",
    "bcrypt": "^5.1.0",
    "buffer": "^6.0.3",
    "caniuse-lite": "^1.0.30001517",
    "cookie": "^0.5.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "svelte-adapter-static-digitalocean": "^2.0.1",
    "svelte-i18n": "^3.7.0",
    "svelte-local-storage-store": "^0.5.0",
    "swiper": "^10.2.0"
  }
}
